{
	"root": false,
	"$schema": "https://biomejs.dev/schemas/2.2.0/schema.json",
	"files": {
		"includes": [
			"package.json",
			"src/**/*.ts",
			"src/**/*.js",
			"src/**/*.json",
			"!**/config.json",
			"!**/setup.json",
			"!**/gsm_operator_cache.json"
		]
	},
	"javascript": {
		"globals": ["Bun"]
	},
	"linter": {
		"rules": {
			"correctness": {
				"noUndeclaredVariables": "off"
			}
		}
	}
}
