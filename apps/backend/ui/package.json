{
	"name": "@belaui/ui",
	"type": "module",
	"dependencies": {
		"@belaui/server": "workspace:*",
		"bootstrap": "^4.6.2",
		"jquery": "^3.7.1",
		"jquery-ui": "~1.12.1"
	},
	"devDependencies": {
		"@types/bootstrap": "^4.6.6",
		"@types/jquery": "^3.5.32",
		"@types/jqueryui": "^1.12.24",
		"typescript": "~5.7.3",
		"vite": "^6.2.0",
		"npm-run-all2": "^7.0.2"
	},
	"scripts": {
		"build": "bun x --bun vite build --mode production",
		"check": "run-p check:*",
		"check:tsc": "tsc",
		"dev": "bun x --bun vite --mode development",
		"fix": "bun x --bun @biomejs/biome check --write",
		"format": "bun x --bun @biomejs/biome format --write",
		"lint": "bun x --bun @biomejs/biome lint --write",
		"preview": "bun x --bun vite preview"
	}
}
